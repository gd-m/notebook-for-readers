<%= form_for @note do |f| -%>
	<%= f.label :content %>
	<%= f.text_field :content %>

	<%= f.label :public, "Public" %>
	<%= f.check_box :public %>

	<br><br>

	<%= f.label :book_id, "Select a Book" %>
	<%= f.collection_select :book_id, Book.all, :id , :name , promp: true %>

	<br><br>


	<%= f.submit %>

<% end -%>